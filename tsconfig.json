{
  "compilerOptions": {
    "module": "CommonJS",
    "lib": ["dom", "es6"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "./",
    "types": ["node"],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["./*.ts", "**/*.ts", "../global.d.ts"]
}
